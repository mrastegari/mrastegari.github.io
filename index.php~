<script language=javascript type='text/javascript'>

function toggleHide(id) 
{
  if (document.getElementById)
  {
    obj = document.getElementById(id);
    if (obj.style.display == "none")
    {
      obj.style.display = "";
    } 
    else 
    {
      obj.style.display = "none";
    }
  }
}  
</script> 



